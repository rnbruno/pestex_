{
    "name": "balajidharma/laravel-media-manager",
    "description": "Laravel media manager",
    "license": "MIT",
    "keywords": ["media","laravel","laravel-media", "media-manager", "laravel-media-manager"],
    "authors": [
        {
            "name": "Balaji Dharmaraja",
            "homepage": "https://github.com/balajidharma"
        }
